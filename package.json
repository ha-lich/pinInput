{
  "name": "admin-panel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@craco/craco": "^6.4.5",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.4",
    "@testing-library/user-event": "^12.0.9",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.62",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.26",
    "antd": "^4.23.3",
    "antd-tag-input": "^1.0.17",
    "async-mutex": "^0.4.0",
    "axios": "^0.27.2",
    "craco-less": "^2.0.0",
    "env-cmd": "^10.1.0",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.4",
    "json-server": "^0.17.3",
    "less": "^4.1.3",
    "msw": "^0.49.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.5",
    "sass": "^1.56.2",
    "source-map-loader": "^4.0.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "webpack": "^5.83.1"
  },
  "scripts": {
    "dev": "env-cmd -f ./.env craco start",
    "build:dev": "env-cmd -f ./.env craco build",
    "staging": "env-cmd -f ./.env craco start",
    "build:staging": "env-cmd -f ./.env craco build",
    "production": "env-cmd -f ./.env craco start",
    "build:production": "env-cmd -f ./.env craco build",
    "lint": "eslint src",
    "lint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "check-types": "tsc --pretty --noEmit",
    "test": "craco test",
    "eject": "craco eject",
    "json-server": "json-server --watch db.json --port 3000",
    "predeploy": "env-cmd -f ./.env craco build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@testing-library/dom": "^7.21.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.0",
    "tsc-files": "^1.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.14"
  }
}
